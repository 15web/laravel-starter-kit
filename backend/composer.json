{
    "name": "15web/laravel-starter-kit",
    "description": "Starter kit for Laravel projects",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "cuyz/valinor": "^1.14",
        "doctrine/dbal": "^4.2",
        "doctrine/migrations": "^3.8",
        "doctrine/orm": "^3.3",
        "gedmo/doctrine-extensions": "^3.19",
        "laravel/framework": "^12.1",
        "laravel/tinker": "^2.10",
        "phpstan/phpdoc-parser": "^2.1",
        "spatie/laravel-route-attributes": "^1.25",
        "symfony/cache": "^7.2",
        "symfony/console": "^7.2",
        "symfony/doctrine-bridge": "^7.2",
        "symfony/property-access": "^7.2",
        "symfony/serializer": "^7.2",
        "symfony/uid": "^7.2",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "brianium/paratest": "^7.8",
        "driftingly/rector-laravel": "^2.0",
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": "^3.70",
        "larastan/larastan": "^3.1",
        "league/openapi-psr7-validator": "^0.22.0",
        "nunomaduro/collision": "^8.6",
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.5",
        "psalm/plugin-laravel": "^3.0",
        "psalm/plugin-phpunit": "^0.19",
        "rector/rector": "^2.0",
        "spatie/laravel-ignition": "^2.9",
        "symfony/psr-http-message-bridge": "^7.2",
        "symfony/yaml": "^7.2",
        "vimeo/psalm": "^6.8",
        "weirdan/doctrine-psalm-plugin": "^2.10"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dev\\": "dev/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    }
}
